package filtroDeFaturas;

public class Fatura {
	
	protected String codigo;
	protected double valor ;
	
	public Fatura(String cod, double val, String data, Cliente clt){
		this.codigo = cod;
		this.valor = val;
	}
	
	public String getCodigo(){
		return codigo;
	}
	
	public double getValor(){
		return valor;
	}
}
